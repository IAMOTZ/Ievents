{
  "name": "ievents",
  "version": "1.0.0",
  "description": "A web app to help manage and run commercial event centers",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/app",
    "build": "babel ./server -d ./build",
    "server:dev": "npm run setUpDb && nodemon ./server/app --exec babel-node  --watch ./server",
    "react:dev": "webpack-dev-server --content-base src --inline --hot",
    "react:pro": "set NODE_ENV=production&& webpack",
    "template:dev": "gulp",
    "setUpDb": "babel-node ./server/setUpDb",
    "test": "set NODE_ENV=test&& babel-node ./server/setUpDb && nyc mocha --compilers js:babel-core/register ./test/**/*.js --timeout 5000 --exit",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "./node_modules/.bin/eslint",
    "heroku-postbuild": "npm run react:pro && npm run build && npm run setUpDb"  
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IAMOTZ/Ievents.git"
  },
  "author": "Ogunniyi Tunmise",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IAMOTZ/Ievents/issues"
  },
  "homepage": "https://github.com/IAMOTZ/Ievents#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0-beta.2",
    "classnames": "^2.2.5",
    "cloudinary": "^1.9.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "image-webpack-loader": "^3.4.2",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-sass": "^4.7.2",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.11.1",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-dropzone": "^4.2.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.22.8",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "browser-sync": "^2.18.13",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0"
  }
}
