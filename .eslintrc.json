{
  "root": true,
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "node": true,
    "es6": true,
    "mocha": true,
    "jest": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "import/no-unresolved": [
      2,
      {
        "caseSensitive": false
      }
    ],
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/label-has-for": 0
  }
}